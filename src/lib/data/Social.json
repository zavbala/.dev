{
	"github": {
		"icon": "fa-github",
		"base": "https://github.com/trending",
		"schema": {
			"topic": "> h2 > a",
			"tagline": "> p",
			"source": "> h2 > a",
			"votes": "a.Link--muted"
		},
		"selector": "article",
		"options": ["", "javascript", "typescript", "python", "rust"],
		"sort": "byPathParams"
	},
	"product-hunt": {
		"gql": {
			"query": "ph",
			"client": "ProductHunt",
			"vars": { "kind": "POPULAR" }
		},
		"icon": "fa-product-hunt",
		"schema": {
			"thumbnail": "https://ph-files.imgix.net/",
			"source": ["https://www.producthunt.com/posts/", "topic", "hyphenate"]
		},
		"entry": "homefeed.edges.0.node.items"
	},
	"indie-hackers": {
		"icon": "fa-hackerrank",
		"base": "https://www.indiehackers.com",
		"schema": {
			"topic": "h3.story__title",
			"votes": "span.story__count-number",
			"source": "> a"
		},
		"selector": "div.homepage-post"
	},
	"hacker-news": {
		"icon": "fa-y-combinator",
		"base": "https://news.ycombinator.com",
		"schema": {
			"topic": "span.titleline > a",
			"source": "span.titleline > a",
			"votes": "span.subline > span.score"
		},
		"selector": "tr.athing",
		"options": ["newest", "show", "ask", "jobs"],
		"sort": "byPathParams"
	},
	"vscode": {
		"icon": "fa-microsoft",
		"base": "https://marketplace.visualstudio.com/getextensionspercategory",
		"args": {
			"Product": "vscode"
		},
		"schema": {
			"topic": "t",
			"source": "l",
			"tagline": "s",
			"thumbnail": "i"
		},
		"entry": "epc.0.e",
		"options": ["featured", "this week", "today", "this month", "popular", "recently added"],
		"sort": "byIndex"
	}
}
